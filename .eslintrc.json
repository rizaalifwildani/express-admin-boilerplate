{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "google"
  ],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "ignorePatterns": [
    "*test.js",
    ".gitignore",
    "."
  ],
  "rules": {
    "no-console": "off",
    "semi": [
      "error",
      "never"
    ],
    "new-cap": ["off", { "capIsNewExceptionPattern": "\\.Bar$" }],
    "max-len": 0
  }
}